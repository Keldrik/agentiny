{
  "name": "@agentiny/core",
  "repository": "https://github.com/Keldrik/agentiny",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.2.0",
  "description": "Lightweight TypeScript agent framework - core package",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [
    "agent",
    "trigger",
    "condition",
    "action",
    "reactive",
    "typescript"
  ],
  "author": "Thomas Wiegold",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.8.1",
    "@vitest/ui": "^3.2.4",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
