{
  "name": "@agentiny/anthropic",
  "version": "0.1.0",
  "description": "Anthropic Claude adapter for @agentiny/core",
  "repository": "https://github.com/Keldrik/agentiny",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "agent",
    "anthropic",
    "adapter",
    "llm",
    "claude"
  ],
  "author": "Thomas Wiegold",
  "license": "MIT",
  "peerDependencies": {
    "@agentiny/core": "*",
    "@anthropic-ai/sdk": "^0.67.0"
  },
  "peerDependenciesMeta": {
    "@agentiny/core": {
      "optional": false
    },
    "@anthropic-ai/sdk": {
      "optional": false
    }
  },
  "devDependencies": {
    "@agentiny/core": "*",
    "@anthropic-ai/sdk": "^0.67.0",
    "@types/node": "^24.8.1",
    "typescript": "^5.9.3"
  }
}
